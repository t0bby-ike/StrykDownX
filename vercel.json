{
  "version": 2,
  "builds": [
    {
      "src": "api/download.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/verify.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "/api/download.js"
    },
    {
      "src": "/api/verify",
      "dest": "/api/verify.js"
    }
  ]
}